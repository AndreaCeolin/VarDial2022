 = function(rMi, rMa){return ~~((Math.random()*(rMa-rMi+1))+rMi);};\nthis.hitTest = function(x1, y1, w1, h1, x2, y2, w2, h2){return !(x1 + w1 &lt; x2 || x2 + w2 &lt; x1 || y1 + h1 &lt; y2 || y2 + h2 &lt; y1);};"}
