{"id": "8790199", "revid": "465169", "url": "https://it.wikipedia.org/wiki?curid=8790199", "title": "Naum Akhiezer", "text": ""}
{"id": "8790203", "revid": "465169", "url": "https://it.wikipedia.org/wiki?curid=8790203", "title": "Aleksander Akhiezer", "text": ""}
