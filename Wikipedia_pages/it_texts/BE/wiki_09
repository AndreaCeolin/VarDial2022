{"id": "6722998", "revid": "93893073", "url": "https://it.wikipedia.org/wiki?curid=6722998", "title": "H\u00fcsem", "text": ""}
{"id": "6723000", "revid": "93893079", "url": "https://it.wikipedia.org/wiki?curid=6723000", "title": "Huesem", "text": ""}
